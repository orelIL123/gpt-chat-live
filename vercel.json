{
  "version": 2,
  "builds": [
    { "src": "api/chat.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/chat", "dest": "api/chat.js" },
    { "src": "/widget.js", "dest": "/public/widget.js" },
    { "src": "/logo/logo.png", "dest": "/public/logo/logo.png" },
    { "src": "/test.txt", "dest": "/public/test.txt" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}
